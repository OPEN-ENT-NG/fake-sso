{
  "name": "fr.wseduc~fake-sso",
  "config": {
    "main": "fr.wseduc.sso.FakeSSO",
    "path-prefix": "sso",
    "port": 8041,
    "csrf-token": {{ csrfToken | default('true') }},
    "sql": true,
    "mongodb": true,
    "host": "https://{{ host }}",
    "ssl": true,
    "app-type" : "SYSTEM",
    {% if overrideTheme %} "override-theme": {{ override2d }}, {% endif %}
    "auto-redeploy": false,
    "userbook-host": "https://{{ host }}",
    "mode": "prod",
    "sso-controllers": [
      {
        "class": "fr.wseduc.sso.controllers.Charlemagne",
        "config": {
          "url": "https://www.ecoledirecte.com",
          "teacherUrl": "https://www.ecoledirecte.com",
          "siteId": "{{ fakessoCharlemagneSiteId | default('') }}",
          "privateKey": "{{ fakessoCharlemagnePrivateKey | default('') }}"
        }
      },
      {% if fakessoLVSCode is defined %}
        {
          "class": "fr.wseduc.sso.controllers.VieScolaireFRController",
          "config": {
            "appli": "{{ fakessoLVSCode }}",
            "connection-timeout": 2000,
            "URL_PROPERTY_END_LVS": "/vsn.main/autoLoginTicketSession/getTicket/",
            "public-key": "{{ fakessoLVSPrivateKey | default('') }}"
          }
        },
      {% endif %}
      {% if fakessoControlers is defined %} {{ fakessoControlers }}, {% endif %}
      {
        "class": "fr.wseduc.sso.controllers.Pronote",
        "config": {}
      }
    ]
  }
}